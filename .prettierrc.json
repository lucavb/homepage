{
    "arrowParens": "always",
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-astro"],
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ],
    "trailingComma": "all",
    "useTabs": false
}
